{
  "version": 2,
  "name": "hackaton-front",
  "alias": ["hackaton-front"],
  "regions": ["fra1"],
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://hackaton-back-delta.vercel.app"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_API_URL": "https://hackaton-back-delta.vercel.app"
    }
  },
  "functions": {
    "app/**/*.js": {
      "runtime": "nodejs18.x"
    },
    "pages/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://hackaton-back-delta.vercel.app/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}